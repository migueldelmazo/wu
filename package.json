{
  "name": "wu",
  "version": "0.0.1",
  "description": "Reactive, functional and declarative framework for webapps",
  "main": "build/index.js",
  "scripts": {
    "postinstall": "babel src --out-dir build",
    "format:js": "esformatter -i --config .formatrc 'src/**/*.js'",
    "test": "react-scripts test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/migueldelmazo/wu.git"
  },
  "keywords": [
    "Wu",
    "Framework",
    "Reactive",
    "Functional programming",
    "Declarative programming"
  ],
  "author": "Miguel del Mazo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/migueldelmazo/wu/issues"
  },
  "homepage": "https://github.com/migueldelmazo/wu#readme",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "lodash-utils": "git+https://github.com/migueldelmazo/lodash-utils.git"
  },
  "devDependencies": {
    "esformatter": "^0.10.0",
    "lodash": "^4.17.21",
    "react-scripts": "^3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.js"
    ]
  }
}
